#include<iostream>
using namespace std;
int main(){
    int s=0;
    int n=1000;
    int key=750;
    int e=n;
    while(s<=e){
        int mid=(s+e)/2;
        if(mid==key){
            return 750;

        }
        else if(key>mid){
            s=mid+1;
        }
        else{
            e=mid-1;
        }
    }
    return 0;
}
